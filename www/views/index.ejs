<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
        <link rel="stylesheet" type="text/css" href="css/index.css">
</head>
<body>
	<div id="imagen">
	        	<div id="datos">
				<span id="c"><span id="cv">0</span><span class="peq">°C</span></span>
       				<span id="p"><span id="pmv">0</span><span class="peq s">ug/m³</span></span>
       				<span id="h"><span id="hv">0</span><span class="peq">%</span></span>
				<span id="lafecha">00·00·00.00:00:00</span>
			</div>
			<img id="i" />

	</div>

	<div class="grafica">
		<canvas id="pChart"></canvas>
	</div>
	<div class="grafica">
		<canvas id="hChart"></canvas>
	</div>
	<div class="grafica">
		<canvas id="cChart"></canvas>
	</div>

	<script type="text/javascript" src="js/jquery-3.2.1.min.js"></script>
	<script type="text/javascript" src="js/Chart.min.js"></script>
	<script type="text/javascript" src="/socket.io/socket.io.js"></script>
	<script>
		// Se pasa a string el objeto, primera carga, se muestra el último documento de la colección
		var datos = <%- JSON.stringify(response) %>,
		f,p,h,c,lafecha;

		function formafecha(lafecha) {
			var fecha = new Date(lafecha);
			var horas = fecha.getHours();
			horas = ("0" + horas).slice(-2);
			var minutos = fecha.getMinutes();
			minutos = ("0" + minutos).slice(-2);
			var segundos = fecha.getSeconds();
			segundos = ("0" + segundos).slice(-2);
			fecha = fecha.getDate() +'·'+ (fecha.getMonth() + 1) +'·'+ fecha.getFullYear().toString().substr(-2) +'.'+ horas +':'+ minutos +':'+ segundos;
			return fecha;
		}

		if (datos.error) {
			console.log('error');
		} else {
			f = ( !datos.message ) ? "" : datos.message.f;
			p = ( !datos.message ) ? "0" : datos.message.p;
			h = ( !datos.message ) ? "0" : datos.message.h;
			c = ( !datos.message ) ? "0" : datos.message.c;
			lafecha = ( !datos.message ) ? "00·00·00.00:00:00" : formafecha(datos.message.createdAt);

			$('#i').attr('src','data:image/jpg;base64,'+ f);
			$('#pmv').html(p);
			$('#hv').html(h);
			$('#cv').html(c);
			$('#lafecha').html(lafecha);
		}
		

		var options = { maintainAspectRatio: false };

		// Gráfica pm
		var pconfig = {
			type: 'line',
			options: options,
			data: {
				labels: [lafecha],
				datasets: [{
					label: "PM 2.5",
					data: [p],
					fill: true,
					"borderColor":"rgb(234, 92, 92)",
					"backgroundColor":"rgb(234, 92, 92, 0.3)"
				}]
			}
		};

		var ctx1 = document.getElementById("pChart").getContext("2d");
		var pChart = new Chart(ctx1, pconfig);

		// Gráfica humedad
		var hconfig = {
			type: 'line',
			options: options,
			data: {
				labels: [lafecha],
 				datasets: [{
					label: "Humedad",
					data: [h],
					fill: true,
					"borderColor":"rgb(75, 192, 192)",
					"backgroundColor":"rgb(75, 192, 192, 0.3)"
				}]
			}
		};

		var ctx2 = document.getElementById("hChart").getContext("2d");
		var hChart = new Chart(ctx2, hconfig);


		// Gráfica grados
		var cconfig = {
			type: 'line',
			options: options,
			data: {
				labels: [lafecha],
				datasets: [{
					label: "°Celsius",
					data: [c],
					fill: true,
					"borderColor":"rgb(234, 219, 92)",
					"backgroundColor":"rgb(234, 219, 92, 0.3)"

 				}]
			}
		};

		var ctx3 = document.getElementById("cChart").getContext("2d");
		var cChart = new Chart(ctx3, cconfig);


		var cuentavueltas = 0;

		// Socket, actualizar, foto, datos y gráficas.
		var socket = io.connect();
		socket.on('a', function (d) {
			$('#i').attr('src','data:image/jpg;base64,'+ d.f[0]);
			$('#cv').html(d.c);
			$('#pmv').html(d.p);
			$('#hv').html(d.h);
			var fe = formafecha(d.fe);
			$('#lafecha').html(fe);

			// actualiza gráfica pm
			pconfig.data.labels.push(fe);
			pconfig.data.datasets[0].data.push(d.p);
			pChart.update();

			// actualiza gráfica h
			hconfig.data.labels.push(fe);
			hconfig.data.datasets[0].data.push(d.h);
			hChart.update();

			// actualiza gráfica c
			cconfig.data.labels.push(fe);
			cconfig.data.datasets[0].data.push(d.c);
			cChart.update();

			
			if ( cuentavueltas > 4 ) {
				pconfig.data.labels.shift();
				hconfig.data.labels.shift();
				cconfig.data.labels.shift();
				pconfig.data.datasets[0].data.shift();
				hconfig.data.datasets[0].data.shift();
				cconfig.data.datasets[0].data.shift();
				pChart.update();
				hChart.update();
				cChart.update();
			}

			cuentavueltas++;


		});

</script>

</body>
</html>
